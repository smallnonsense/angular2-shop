 <?xml version="1.0" encoding="UTF-8"?>
    <configuration>
      <system.webServer>
        <rewrite>
          <rules>
            <rule name="Angular" stopProcessing="true">
              <match url=".*" />
              <conditions logicalGrouping="MatchAll">
              </conditions>
              <action type="Rewrite" url="/"  appendQueryString="true" />
            </rule>
          </rules>
        </rewrite>
      </system.webServer>
    </configuration>