// Place your settings in this file to overwrite default and user settings.
{
    "editor.renderWhitespace": "all",
    "editor.codeLens": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.fontFamily": "Consolas, 'Courier New', monospace",
    "editor.fontSize": 12,
    "files.exclude": {
        "node_modules/**": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    }
}